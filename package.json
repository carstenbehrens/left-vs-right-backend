    "cloudinary": "^1.23.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.9.0",
    "mongoose": "^5.11.5",
    "newsapi": "^2.4.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^26.4.4",
    "typedi": "^0.8.0"
  }
